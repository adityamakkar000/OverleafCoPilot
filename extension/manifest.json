{
  "name": "Overleaf Copilot",
  "version": "1.0.0",
  "description": "Overleaf Copilot",
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
 "content_scripts": [
      {
          "matches": ["https://*.overleaf.com/project/*"],
          "run_at": "document_end",
          "js": ["copyv2.js"],
          "all_frames": true
      }
  ]
}